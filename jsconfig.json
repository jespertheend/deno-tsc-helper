{
	"extends": "./.denoTypes/tsconfig.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"target": "esnext",
		"checkJs": true,
		"module": "esnext",
		"useDefineForClassFields": true,
		"strict": true,
		"exactOptionalPropertyTypes": true
	},
	"include": [
		"**/*.js",
		"**/*.ts",
	]
}
